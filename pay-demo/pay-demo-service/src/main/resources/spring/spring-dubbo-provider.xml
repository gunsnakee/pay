<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"	
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
		default-lazy-init="true">
	<description>service模块 dubbo 注册 --农剑斌 2014-10-18</description>
    <!-- 声明需要暴露的服务接口 -->
    <dubbo:service interface="com.hlpay.base.api.PayAccountUserApi" ref="payAccountUserApi" />
    <!-- 和本地bean一样实现服务-->
    <bean id="payAccountUserApi" class="com.hlpay.base.api.impl.PayAccountUserApiImpl" /> 
    <!-- 声明需要暴露的服务接口 -->
    <dubbo:service interface="com.hlpay.base.api.PaySysDataDicApi" ref="paySysDataDicApi" />
    <!-- 和本地bean一样实现服务--> 
    <bean id="paySysDataDicApi" class="com.hlpay.base.api.impl.PaySysDataDicApiImpl" />
   
</beans>